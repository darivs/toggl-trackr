{
  "name": "toggl-trackr",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm dev:server\" \"pnpm dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsx server/index.ts",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "pnpm run lint:client && pnpm run lint:server && pnpm run lint:eslint && pnpm run lint:prettier",
    "lint:client": "tsc --noEmit",
    "lint:server": "tsc --noEmit -p tsconfig.server.json",
    "lint:eslint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs",
    "lint:prettier": "prettier --check \"**/*.{js,ts,tsx,css,md,json}\"",
    "format": "pnpm run format:eslint && pnpm run format:prettier",
    "format:eslint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "format:prettier": "prettier --write \"**/*.{js,ts,tsx,css,md,json}\"",
    "start": "node dist/server/index.js"
  },
  "dependencies": {
    "@types/node": "^22.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "lucide-react": "^0.372.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^15.11.0",
    "postcss": "^8.4.47",
    "prettier": "^3.4.1",
    "tailwindcss": "^3.4.14",
    "typescript-eslint": "^8.54.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5",
    "vite": "^5.4.8"
  }
}
